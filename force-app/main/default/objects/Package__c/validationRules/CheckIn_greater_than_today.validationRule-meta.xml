<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CheckIn_greater_than_today</fullName>
    <active>true</active>
    <errorConditionFormula>Check_In__c   &lt;   NOW()</errorConditionFormula>
    <errorMessage>Check IN date should be today or in the future</errorMessage>
</ValidationRule>
